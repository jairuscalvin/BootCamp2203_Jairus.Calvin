<div class="container">
    <h1>This Is Home Pages</h1>
    <h3>Welcome Home Dear <%= nama %>! </h3>
    <div class="list">
        <h3>Kontak List Anda: </h3>
        <% if (kontak.length < 1) { %>
            <p>Data Kosong</p>
        <% }else{ %>
        <% kontak.forEach(kontak => { %>
            <ul>
                <li>No. <%= index++ %> </li>
                <li>Nama : <%= kontak.name %> </li>
                <li>Email : <%= kontak.email %> </li>
                <li>No HP : <%= kontak.phone %> </li>
            </ul>
        <% }) %>
        <% } %> 
    </div>
</div>
